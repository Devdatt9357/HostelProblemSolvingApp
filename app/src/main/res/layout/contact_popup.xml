<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardContact2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/contact_pop_height"
        app:cardBackgroundColor="@color/colorPrimary"
        app:cardCornerRadius="@dimen/cornerRadius">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView43"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/ripple_effect"
                android:clickable="true"
                app:layout_constraintBottom_toTopOf="@+id/guideline226"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView44"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/ripple_effect"
                android:clickable="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView43" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline218"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.12" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline113"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.9" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline221"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".7886" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline226"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".5" />

            <ImageView
                android:id="@+id/imageView42"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/guideline220"
                app:layout_constraintEnd_toStartOf="@+id/guideline218"
                app:layout_constraintStart_toStartOf="@+id/guideline215"
                app:layout_constraintTop_toTopOf="@+id/guideline220"
                app:srcCompat="@drawable/ic_phone" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline215"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.057" />

            <TextView
                android:id="@+id/textView54"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/product_sans_bold"
                android:text="Call us"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="@dimen/call"
                app:layout_constraintStart_toStartOf="@+id/guideline215"
                app:layout_constraintTop_toTopOf="@+id/guideline216" />

            <TextView
                android:id="@+id/textView57"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:fontFamily="@font/product_sans"
                android:maxLines="1"
                android:text="devdattchavda1692000@gmail.com"
                android:textColor="@color/colorPrimaryDark2"
                android:textSize="@dimen/descript"
                app:layout_constraintBottom_toTopOf="@+id/guideline221"
                app:layout_constraintEnd_toStartOf="@+id/guideline113"
                app:layout_constraintStart_toStartOf="@+id/guideline219"
                app:layout_constraintTop_toTopOf="@+id/guideline221" />

            <TextView
                android:id="@+id/textView56"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/product_sans"
                android:text="+917405339670"
                android:textColor="@color/colorPrimaryDark2"
                android:textSize="@dimen/descript"
                app:layout_constraintBottom_toTopOf="@+id/guideline220"
                app:layout_constraintStart_toStartOf="@+id/guideline219"
                app:layout_constraintTop_toTopOf="@+id/guideline220" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline217"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".5486" />

            <TextView
                android:id="@+id/textView55"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/product_sans_bold"
                android:text="Mail us"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="@dimen/call"
                app:layout_constraintStart_toStartOf="@+id/guideline215"
                app:layout_constraintTop_toTopOf="@+id/guideline217" />

            <ImageView
                android:id="@+id/imageView41"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toTopOf="@+id/guideline221"
                app:layout_constraintEnd_toStartOf="@+id/guideline218"
                app:layout_constraintStart_toStartOf="@+id/guideline215"
                app:layout_constraintTop_toTopOf="@+id/guideline221"
                app:srcCompat="@drawable/ic_email" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline220"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".3543" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline216"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".1143" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/guideline219"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.1457" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</RelativeLayout>